{
  "name": "addone-otp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "analyze": "cross-env ANALYZE=1 umi dev",
    "start:nomock": "cross-env MOCK=none umi dev",
    "start:prod.addone": "cross-env UMI_ENV=prod.addone umi dev",
    "build": "cross-env UMI_ENV=prod umi build",
    "build:local": "cross-env UMI_ENV=local umi build",
    "build:dev": "cross-env UMI_ENV=dev umi build",
    "build:test": "cross-env UMI_ENV=test umi build",
    "build:local.ssr": "cross-env UMI_ENV=local.ssr umi build",
    "build:prod.ssr": "cross-env UMI_ENV=prod.ssr umi build",
    "build:prod.cdn": "cross-env UMI_ENV=prod.cdn umi build",
    "build:prod.addone": "cross-env UMI_ENV=prod.addone umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@hocgin/browser-addone-kit": "^2.0.29",
    "@hocgin/hkit": "^2.0.29",
    "@hocgin/ui": "^5.0.17",
    "@types/qrcode": "^1.5.0",
    "antd": "^5.1.2",
    "classnames": "^2.3.1",
    "jquery": "^3.6.3",
    "js-web-screen-shot": "^1.8.2",
    "jssha": "^3.3.0",
    "qr-scanner": "^1.4.2",
    "qrcode": "^1.5.1",
    "query-string": "^7.0.1",
    "react": "18.x",
    "react-dom": "18.x",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@hocgin/mock-kit": "^2.0.29",
    "@hocgin/umijs-plugin-browser-addone": "^1.0.93",
    "@types/jquery": "^3.5.16",
    "@types/mockjs": "^1.0.6",
    "@types/react": "^18.x",
    "@types/react-dom": "^18.x",
    "@types/uuid": "^9.0.0",
    "@umijs/plugin-qiankun": "^2.37.2",
    "@umijs/preset-react": "^1.8.30",
    "@umijs/test": "^3.5.20",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.0.7",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "umi": "^3.5.20",
    "yorkie": "^2.0.0"
  }
}
